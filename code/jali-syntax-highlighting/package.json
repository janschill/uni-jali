{
    "name": "jali-syntax-highlighting",
    "displayName": "JaLi Syntax Highlighting",
    "description": "Syntax highlighting for the functional language JaLi",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.44.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "jali",
            "aliases": ["JaLi", "jali"],
            "extensions": [".jali"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "jali",
            "scopeName": "source.jali",
            "path": "./syntaxes/jali.tmLanguage.json"
        }]
    }
}